                                <Code>07081012</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="UpwardEmission" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>emissione diretta verso l'alto dell'apparecchio</Class>
                                <Label>PL_AP_EM</Label>
                                <Code>07081013</Code>
                                <Definition/>
