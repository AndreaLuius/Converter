    <xsd:complexType name="LightSpotEquipmentType">
        <xsd:complexContent>
            <xsd:extension base="InstanceMetadataType">
                <xsd:sequence>
                    <xsd:element ref="gml:Point" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>Posizione</Class>
                                <Label>PL_IS_POS</Label>
                                <Code>070809101</Code>
                                <Definition>corrisponde alla localizzazione geografica
                                    dell'installazione</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="EquipmentType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>tipologia installazione</Class>
                                <Label>PL_IS_SOS</Label>
                                <Code>07080904</Code>
                                <Definition>indica la tipologia di installazione</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Height" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>altezza apparecchio</Class>
                                <Label>PL_IS_ALT</Label>
                                <Code>07080905</Code>
                                <Definition>altezza dalla sede stradale [m]</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Incline" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>inclinazione</Class>
                                <Label>PL_IS_TILT</Label>
                                <Code>07080906</Code>
                                <Definition>inclinazione rispetto all'asse orizzontale della sede
                                    stradale [gradi]</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element ref="Distance" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>distanza sostegno dall'inizio della carreggiata</Class>
                                <Label>PL_IS_CAR</Label>
                                <Code>07080907</Code>
                                <Definition>Indica la distanza tra la base del sostegno e l'inizio
                                    della carreggiata [m]. Per tipologie di sostegno diverse da su
                                    palo o su braccio deve essere valorizzato con valore
                                    nullo</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Length" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>lunghezza braccio</Class>
                                <Label>PL_IS_BRA</Label>
                                <Code>07080908</Code>
                                <Definition>Nel caso di sostegno su braccio, indica la lunghezza di
                                    quest'ultimo [m]. Per tipologie di sostegno diverse da su palo o
                                    su braccio non deve essere valorizzato o valorizzato con valore
                                    nullo </Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Age" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>eta' sostegno</Class>
                                <Label>PL_IS_AGE</Label>
                                <Code>07080909</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="SupportMaterial" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>materiale sostegno</Class>
                                <Label>PL_IS_MAT</Label>
                                <Code>07080910</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="AdditionalDevice" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>altri elementi sul sostegno</Class>
                                <Label>PL_IS_ELE</Label>
                                <Code>07080911</Code>
                                <Definition>indica l'eventuale presenza e la tipologia di ulteriori
                                    elementi sul sostegno</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="AdditionalElement" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>istallazione altri elementi sul palo</Class>
                                <Label>PL_IS_AEP</Label>
                                <Code>07080912</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="SupportStatus" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>stato del sostegno</Class>
                                <Label>PL_IS_STS</Label>
                                <Code>07080913</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

