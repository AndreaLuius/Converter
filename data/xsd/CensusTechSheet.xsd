    <xsd:complexType name="PLSystemGeneralDataType">
        <xsd:complexContent>
            <xsd:extension base="InstanceMetadataType">
                <xsd:sequence>
                    <xsd:element ref="gml:Polygon" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>Estensione</Class>
                                <Label>AN_IP_EXT</Label>
                                <Code>070801102</Code>
                                <Definition>Corrisponde alla componente spaziale della Classe
                                    "090101 Comune" delle regole tecniche sui database
                                    geotopografici (allegato DM 10 novembre 2011)</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="ISTATCode" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>codice istat comune</Class>
                                <Label>AN_IP_IST</Label>
                                <Code>07080101</Code>
                                <Definition>Codice ISTAT del comune a cui afferisce l'anagrafica. Il
                                    codice ISTAT deve appartenere alla lista di codici disponibile
                                    sul sito dell'ISTAT, https://www.istat.it/it/archivio/6789,nella
                                    sezione "Elenco dei codici e delle denominazioni delleunità
                                    territoriali". Come speficificato da ISTAT, ogni codice e' dato
                                    da: Codice Regione + Codice Comune, dove il Codice Comune
                                    èformato da: Codice Provincia + Progressivo del
                                    Comune</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="City" minOccurs="1" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>nome comune</Class>
                                <Label>AN_IP_NOM</Label>
                                <Code>07080102</Code>
                                <Definition>Nome del Comune a cui si riferisce l'anagrafica
                                    dell'illuminazione pubblica (l'elemento puo' essere ripetuto per
                                    specificare il nome in piu' lingue)</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Region" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>regione</Class>
                                <Label>AN_IP_REG</Label>
                                <Code>07080103</Code>
                                <Definition>Regione di appartenenza dell'anagrafica</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="NumberOfCitizens" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>numero di abitanti</Class>
                                <Label>AN_IP_N_AB</Label>
                                <Code>07080104</Code>
                                <Definition>Numero di abitanti</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Surface" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>superficie</Class>
                                <Label>AN_IP_SUP</Label>
                                <Code>07080105</Code>
                                <Definition>Superficie complessiva servita dall'anagrafica
                                    [km2]</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="PersonInCharge" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>responsabile comunale</Class>
                                <Label>AN_IP_RE_C</Label>
                                <Code>07080106</Code>
                                <Definition>Cognome e nome del responsabile comunale
                                    dell'anagrafica</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Overseer" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>nome gestore</Class>
                                <Label>AN_IP_GEST</Label>
                                <Code>07080107</Code>
                                <Definition>Nome del gestore dell'anagrafica</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="ForfaitSystemFlag" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>gestione impianto a forfait</Class>
                                <Label>AN_IP_GE_F</Label>
                                <Code>07080108</Code>
                                <Definition>Discrimina le anagrafiche con impianti di illuminazione
                                    pubblica gestiti a forfait</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="NumberOfConnectedLightSpots" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>punti luce totali</Class>
                                <Label>AN_IP_N_PL</Label>
                                <Code>07080109</Code>
                                <Definition>Numero di punti luce associati
                                    all'anagrafica</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="NumberOfOwnedLightSpots" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>numero di punti luce di proprietà</Class>
                                <Label>AN_IP_N_PR</Label>
                                <Code>07080110</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    <xsd:complexType name="PODConsumptionDataType">
        <xsd:complexContent>
            <xsd:extension base="InstanceMetadataType">
                <xsd:sequence>
                    <xsd:element ref="ReportingYear" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>anno di riferimento consumi</Class>
                                <Label>POD_C_DAT</Label>
                                <Code>07080302</Code>
                                <Definition>indica l'anno di riferimento sulla base del quale sono
                                    compilati i campi relativi ai consumi (anno
                                    precedente)</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="ContractType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>tipologia contratto</Class>
                                <Label>POD_C_TY</Label>
                                <Code>07080303</Code>
                                <Definition>indica la tipologia di contratto stipulata dal Comune</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="DistributorName" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>denominazione del distributore</Class>
                                <Label>POD_C_DIS</Label>
                                <Code>07080304</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="SupplierName" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>denominazione del fornitore</Class>
                                <Label>POD_C_FOR</Label>
                                <Code>07080305</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="InstalledPower" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>potenza installata</Class>
                                <Label>POD_C_KWI</Label>
                                <Code>07080306</Code>
                                <Definition>indica la potenza [kW] installata a livello di POD al
                                    lordo delle perdite di rete e degli ausiliari
                                    dell'impianto</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="PreviousYearAnnualConsumption" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>consumo annuale (anno precedente)</Class>
                                <Label>POD_C_CON</Label>
                                <Code>07080307</Code>
                                <Definition>[kWh/anno]</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="PreviousYearElectricEnergyAnnualAmount" minOccurs="1"
                        maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>costo annuale (anno precedente) per la sola fornitura di
                                    energia elettrica per pubblica illuminazione</Class>
                                <Label>POD_C_COS</Label>
                                <Code>07080308</Code>
                                <Definition>€/anno iva esclusa</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="ContractuallyCommittedPower" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>potenza contrattuale impegnata</Class>
                                <Label>POD_C_KWC</Label>
                                <Code>07080309</Code>
                                <Definition>Rappresenta la potenza contrattualmente impegnata [kW],
                                    ossia il livello di potenza indicato nei contratti e reso
                                    disponibile dal fornitore</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="PowerFactor" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>cosφ tipico</Class>
                                <Label>POD_C_COST</Label>
                                <Code>07080310</Code>
                                <Definition>Fattore di potenza del sistema elettrico a corrente
                                    alternata</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="CurrentYearAnnualConsumption" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>consumo annuale (anno attuale)</Class>
                                <Label>POD_C_CN_A</Label>
                                <Code>07080311</Code>
                                <Definition>[kWh/anno]</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="CurrentYearElectricEnergyAnnualAmount" minOccurs="1"
                        maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>costo annuale (anno attuale), per la sola fornitura di
                                    energia elettrica per pubblica illuminazione</Class>
                                <Label>POD_C_CS_A</Label>
                                <Code>07080312</Code>
                                <Definition>€/anno iva esclusa</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="PODGeneralDataType">
        <xsd:complexContent>
            <xsd:extension base="InstanceMetadataType">
                <xsd:sequence>
                    <xsd:element ref="gml:Point" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>Posizione</Class>
                                <Label>POD_A_POS</Label>
                                <Code>070802101</Code>
                                <Definition>corrisponde alla localizzazione geografica del
                                    POD</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Toponym" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>toponimo stradale pod</Class>
                                <Label>POD_A_TOP</Label>
                                <Code>07080202</Code>
                                <Definition>Nome della strada, via o piazza o largo, etc.,
                                    comprensivo di apposizione. Unitamente all'attributo "07080203 -
                                    POD_RIF" (elemento StreetNumber) compone
                                    l'indirizzo</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="LocationReference" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>riferimento localizzazione</Class>
                                <Label>POD_A_RIF</Label>
                                <Code>07080203</Code>
                                <Definition>riporta l'eventuale numero civico associato al POD o un
                                    testo che ne descriva la prossimita' ad un oggetto territoriale
                                    riconosciuto</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="DetectionDate" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>data del rilievo</Class>
                                <Label>POD_A_RIL</Label>
                                <Code>07080204</Code>
                                <Definition>indica la data in cui e' stato effettuato il rilievo
                                    della situazione del POD in situ</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="VarietyFlag" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>promiscuita'</Class>
                                <Label>POD_A_PR</Label>
                                <Code>07080205</Code>
                                <Definition>Indica la presenza di situazioni di promiscuita'
                                    elettrica (true = presenza di situazioni di promiscuita', false
                                    = assenza di situazioni di promisquita')</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="NumberOfElectricPanels" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>numero quadri elettrici</Class>
                                <Label>POD_A_N_Q</Label>
                                <Code>07080206</Code>
                                <Definition>numero quadri elettrici afferenti il POD</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="NumberOfElectricPanelsToBeChanged" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>numero quadri elettrici da sostituire</Class>
                                <Label>POD_A_N_QS</Label>
                                <Code>07080207</Code>
                                <Definition>numero quadri elettrici afferenti il POD da
                                    sostituire</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="NumberOfElectricPanelsToBeReconditioned" minOccurs="1"
                        maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>numero quadri elettrici da ricondizionare</Class>
                                <Label>POD_A_N_QR</Label>
                                <Code>07080208</Code>
                                <Definition>numero quadri elettrici afferenti il POD da
                                    ricondizionare</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="PODPreservationStatus" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>stato di conservazione del manufatto contenente il
                                    pod</Class>
                                <Label>POD_A_MAN</Label>
                                <Code>07080209</Code>
                                <Definition>indica lo stato di conservazione del manufatto che
                                    contiene il POD</Definition>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="CustomerID" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                <Class>codice cliente</Class>
                                <Label>POD_A_CLIE</Label>
                                <Code>07080210</Code>
                                <Definition/>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    <xsd:complexType name="PODType">
        <xsd:sequence>
            <xsd:element ref="PODCode" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        <Class>codice pod</Class>
                        <Label>POD_COD</Label>
                        <Code>07080201 </Code>
                        <Definition>indica il codice identificativo del POD (Point Of Delivery)
                            considerato; il codice POD e' un codice composto da 14 caratteri che
                            identifica in modo certo il punto fisico sulla rete di distribuzione nel
                            territorio nazionale in cui l'energia viene consegnata dal fornitore e
                            prelevata dal cliente finale</Definition>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element ref="PODGeneralData" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        <Class>POD - Anagrafica del punto di prelievo (POD)
                            dell'elettricita'.</Class>
                        <Label>POD_A</Label>
                        <Code>070802</Code>
                        <Definition>Il POD (Point Of Delivery - punto di prelievo dell'elettricita')
                            identifica in modo certo il punto fisico sulla rete di distribuzione nel
                            territorio nazionale in cui l'energia viene consegnata dal fornitore e
                            prelevata dal cliente finale. e' identificato con un codice alfanumerico
                            (14 caratteri) che serve ad individuare con precisione l'utenza. Il POD
                            e' il punto di riferimento per la contabilizzazione del consumo di
                            energia elettrico. In un comune possono esserci più POD, in relazione
                            alle dimensioni del Comune. Ad ogni POD possono essere associati 1 o più
                            quadri elettrici</Definition>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element ref="PODConsumptionData" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        <Class>Pod - consumi</Class>
                        <Label>POD_C</Label>
                        <Code>070803</Code>
                        <Definition>Definisce i consumi del POD. Classe priva di componente
                            spaziale, essendo relazionata agli oggetti contenuti nella Classe POD_A
                            - 070802</Definition>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
